{
  "name": "esp32",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pio run",
        "cwd": "apps/esp32"
      }
    },
    "upload": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pio run --target upload",
        "cwd": "apps/esp32"
      }
    },
    "ota": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pio run -t upload -e esp32dev --upload-port Heizbox.local",
        "cwd": "apps/esp32"
      }
    },
    "monitor": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pio device monitor",
        "cwd": "apps/esp32"
      }
    }
  },
  "tags": []
}